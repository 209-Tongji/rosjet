<?xml version="1.0"?>
<robot name="Jet">
  
<link name="base_link">
  <visual>
    <geometry>
      <box size=".25 .28 .05"/>
    </geometry>
       <material name="green">
        <color rgba="0 .8 .0 .8"/>
    </material>
  </visual>
  <collision>
    <geometry>
      <box size=".25 .28 .05"/>
    </geometry>
   </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1e-3" ixy="0.0" ixz="0.0" iyy="1e-3" iyz="0.0" izz="1e-3"/>
    </inertial>
</link>

  <link name="right_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.1"/>
      </geometry>
       <origin rpy="0 1.57075 0" xyz="0 0 0"/>
       <material name="gray">
        <color rgba=".1 .1 .1 1"/>
    </material>
    </visual>
  <collision>
    <geometry>
        <cylinder length="0.03" radius="0.1"/>
    </geometry>
   </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1e-3" ixy="0.0" ixz="0.0" iyy="1e-3" iyz="0.0" izz="1e-3"/>
    </inertial>
  </link>
  
  <link name="left_wheel">
    <visual>
      <geometry>
        <cylinder length="0.03" radius="0.1"/>
      </geometry>
       <origin rpy="0 1.57075 0" xyz="0 0 0"/>
       <material name="gray">
        <color rgba=".1 .1 .1 1"/>
    </material>
    </visual>
      <collision>
    <geometry>
        <cylinder length="0.03" radius="0.1"/>
    </geometry>
   </collision>
    <inertial>
      <mass value="10"/>
      <inertia ixx="1e-3" ixy="0.0" ixz="0.0" iyy="1e-3" iyz="0.0" izz="1e-3"/>
    </inertial>
  </link>
  
  <joint name="base_to_right_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel"/>
    <origin xyz="0.15 .05 -.03"/>
    <axis xyz="1 0 0"/>
  </joint>
  
  <joint name="base_to_left_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel"/>
    <origin xyz="-0.15 .05 -.03"/>
    <axis xyz="1 0 0"/>
  </joint>
</robot>
